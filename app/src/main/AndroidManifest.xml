<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.hitech.com.hitechcrm">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:theme="@style/LoginTheme" />
        <activity android:name=".AddLead" />
        <activity android:name=".ViewLead" />
        <activity
            android:name=".Task.Locca"
            android:theme="@style/withoutactionbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Navigation_package.Nav_drawer"
            android:label="Home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Task.CreateTask"></activity>
        <activity android:name=".Navigation_package.Customdialog"></activity>
    </application>

</manifest>